{
  "name": "{{ project_slug }}",
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    {% if css_framework == 'None' %}"bs-css",{% endif -%}
    "reason-react"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-react-testing-library",
    "bs-webapi"
  ],
  "ppx-flags": [],
  "refmt": 3,
  "warnings": {
    "error": "+5"
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-open Belt"
  ]
}